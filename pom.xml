			<id>mdobacz</id>
			<name>Micha≈Ç Dobaczewski</name>
			<email>michal (dot) d08 (at) Google mai l</email>
			<organization>unaffiliated</organization>
			<roles>
				<role>made this ages ago</role>
			</roles>
		</developer>
	</developers>
		
</project>
